{
    "manifest_version": 3,
    "name": "Apollo Export",
    "version": "0.01",
    "permissions": [
      "storage",
      "activeTab",
      "geolocation",
      "scripting"
    ],
    "host_permissions":["https://app.apollo.io/*"],
    "background":{
        "service_worker":"bg.js"
    },
    "action": {
      "default_popup": "index.html"
    }
  }
  